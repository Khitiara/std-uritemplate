{
  "Handling of boolean values" :
  {
    "variables": {
       "foo"   : true,
       "bar" : false
     },
     "testcases" : [
        ["{foo}", "true"],
        ["{bar}", "false"]
     ]
  },
  "Language native date-time format" :
  {
    "variables": {
       "nativedate"   : "2023-02-13T18:49:00Z"
     },
     "testcases" : [
        ["{+nativedate}", "2023-02-13T18:49:00Z"],
        ["{nativedate}", "2023-02-13T18%3A49%3A00Z"]
     ]
  }
}
